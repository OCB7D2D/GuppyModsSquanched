    <block name="guppyDesertAmbianceBlock">
      <property name="Extends" value="cropsGrowingMaster" param1="CustomIcon"/>
      <property name="CreativeMode" value="Player"/>
      <property name="UnlockedBy" value="perkLivingOffTheLand,plantedAloe1Schematic"/>
      <property name="Material" value="Mcorn"/>
      <property name="Shape" value="ModelEntity"/>
      <property name="Model" value="#@modfolder:Resources/guppyDesertAmbianceBlock.unity3d?guppyDesertAmbianceBlock.prefab"/>
