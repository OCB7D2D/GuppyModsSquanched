<configs>
	<append xpath="/blocks">

    <block name="guppyWorkingClock">
      <property name="Class" value="ClockDMT, SCore"/>
      <property name="Material" value="Mmetal_weak"/>
      <property name="Place" value="TowardsPlacer"/>
      <property name="Shape" value="ModelEntity"/>
      <property name="PlaceEverywhere" value="true"/>
      <property name="Model" value="#@modfolder:Resources/guppyWorkingClock.unity3d?guppyWorkingClock.prefab"/>
      <property name="ImposterExchange" value="imposterBlock" param1="9"/>
      <property class="RepairItems"> 
	  <property name="resourceSpring" value="1"/> </property>
      <drop event="Harvest" name="resourceSpring" count="1" prob="1" tag="allHarvest"/>
      <drop event="Destroy" count="0"/>
      <drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
      <property name="CustomIcon" value="guppyWorkingClock" /> 
      <property name="CanPickup" value="true" />      
    </block>	
    
