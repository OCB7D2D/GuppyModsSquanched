<configs>
	<append xpath="/blocks">

<block name="guppyTreeBroadleafSummerLg">
	<property name="Extends" value="treeMaster"/>
      <property name="Model" value="#@modfolder:Resources/guppycurSpeedtrees.unity3d?guppyTreeBroadleafSummerLg.prefab"/>
	<property name="IsDistantDecoration" value="false"/>
	<property name="ModelOffset" value="0,0,0"/>
	<property name="MultiBlockDim" value="1,19,1"/>
	<property name="BigDecorationRadius" value="2"/>
	<property name="CanPlayersSpawnOn" value="false"/>
	<property name="ParticleOnDeath" value="treeGib_small_dust"/>
	<drop event="Harvest" name="resourceWood" count="30" tag="oreWoodHarvest"/>
	<property name="SortOrder2" value="0120"/> 
</block>
