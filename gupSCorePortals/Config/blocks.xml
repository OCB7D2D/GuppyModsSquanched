<configs>

	<remove xpath="/blocks/block[@name='portalMaster']/property[@class='RepairItems']"/>
	<remove xpath="/blocks/block[@name='portalMaster']/drop[@event='Harvest']"/>
	<remove xpath="/blocks/block[@name='portalMaster']/drop[@event='Destroy']"/>
	
	<append xpath="/blocks/block[@name='portalMaster']">
		<property class="RepairItems">
			<property name="resourceForgedSteel" value="12"/>
		</property>
		<drop event="Harvest" name="resourceForgedSteel" count="15,28" tag="allHarvest"/>
		<drop event="Destroy" count="0"/>
	</append>
	
